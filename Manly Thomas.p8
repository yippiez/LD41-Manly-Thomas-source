function move(o,b)
if o.selected then
if btn(1) then o.y += 1 end
if btn(0) then o.y -= 1 end
else
do_ai(o,b)

end
end



function do_ai(p, b)
  if (b.y < p.y + p.h/2) then
     p.y -= 1
  elseif (b.y > p.y + p.h/2) then
     p.y += 1
  end
end

function make_ball(x,y)
local b = {}
b.x = x
b.y = y
b.i = 3
b.dx = 2
b.dy = 2
b.w = 2
b.h = 2
add(balls,b)
return b
end

function make_block(x,y)
local b = {}
b.x = x
b.y = y
b.i = 47
b.w = 6
b.h = 6
b.health = 2
add(blocks,b)
return b
end

function collide_mouse(c2,x)
 if stat(32) > c2.x + c2.w then
 			return false
 			end
 if stat(32) + x < c2.x then
 			return false
 			end
 if stat(33) > c2.y + c2.h then
 			return false
 			end
 if stat(33) + x < c2.y then
 			return false
 			end
	return true
end

function collide(c1,c2)
 if c1.x  > c2.x + c2.w then--left
 			return false
 			end
 if c1.x + c1.w < c2.x then--right
 			return false
 			end
 if c1.y  > c2.y + c2.h then --up
 			return false
 			end
 if c1.y + c1.h < c2.y then
 			return false
 			end
	return true
end


function block_update(o)
if o.health <= 0 then del(blocks,o) end
end

function make_area(x,y)
local a = {}
a.x = x*8
a.y = y*8
a.w = 4
a.cando = true
a.h = 4
add(area,a)
return a
end


function draw_area(o)
  local skin = 32
  if menu == true then skin = 33 else skin = 32 end
  if o.cando == true then
  spr(skin,o.x,o.y)
  end
end


function intable(tbl, item)
    for key, value in pairs(tbl) do
        if value == item then return true end
    end
    return false
end


function make_enemy(x,y)
local e = {}
local velvet = flr(rnd(2)) + 1
if velvet == 1 then e.side = false end
if velvet == 2 then e.side = true end
e.state = "normal" --normal,angry,shooting
e.x = x
e.y = y
e.health = 4
e.i = 55
e.w = 4
e.h = 4
add(enemies,e)
return e
end

function cmap(o)
	local ct=false
	-- if colliding with map tiles
		local x1=o.x/8
		local y1=o.y/8
		local x2=(o.x+7)/8
		local y2=(o.y+7)/8
 	local a=fget(mget(x1,y1),0)
 	local b=fget(mget(x1,y2),0)
 	local c=fget(mget(x2,y2),0)
 	local d=fget(mget(x2,y1),0)
 	ct=a or b or c or d

 return ct
end

function cmap2(x,y)
	local ct=false
	-- if colliding with map tiles
		local x1=x/8
		local y1=y/8
		local x2=(x+7)/8
		local y2=(y+7)/8
 	local a=fget(mget(x1,y1),0)
 	local b=fget(mget(x1,y2),0)
 	local c=fget(mget(x2,y2),0)
 	local d=fget(mget(x2,y1),0)
 	ct=a or b or c or d

 return ct
end



function enemy_update(o)
 --e.state normal angry shooting
local random = flr(rnd(30)) + 1
local isdone = false

if random == 3 then
if (o.state == "normal" and isdone == false) then  o.state = "angry" o.i = 56 isdone = true end
if (o.state == "angry" and isdone == false) then  o.state = "shooting" o.i = 57 isdone = true end
if (o.state == "shooting" and isdone == false) then for i=1,flr(rnd(4)+2) do enemy_shoot(o.x,o.y,o.side) end o.state = "normal" o.i = 55 isdone = true end
end
end


function enemy_shoot(x,y,s)
local b = {}
b.x = x
b.y = y
b.w = 2
b.h = 2
b.spre = 0.3
b.dx = rnd(3)+1
b.dy = rnd(b.spre) -b.spre/2
if s == false then b.dx = b.dx end
if s == true then b.dx = -b.dx end
add(bullets,b)
return b
end

function check_area()
	for i in all(area) do 
		if i.cando == true then return false end
			
	end
return true
end


function make_tower(x,y,t)
local b = {}
b.x = x
b.y = y
b.t = t
b.w = 4
b.timer = 0
b.h = 4
if b.t == "b" then
b.i = 67

end
if b.t == "t" then
b.i = 68
end
b.side = false
b.side2 = false
add(towers,b)
return b
end


function mouse_block()
	for b in all(blocks) do
	if collide_mouse(b,4) then return false end
	end
return true
end


function update_towers(o)
	local xs = 0
	local ys = 0
	if o.t == "b" then 	
	o.timer += 1
	if o.timer > 30 then 
	
	for e in all(enemies) do 
		local xs1 = 0
		local ys1 = 0
		if flr((o.x)/8) < flr((e.x)/8) then xs1 = 2 end
	if flr((o.y)/8) < flr((e.y)/8) then ys1 = 2 end
	if flr((o.x)/8) > flr((e.x)/8) then xs1 = -2 end
	if flr((o.y)/8) > flr((e.y)/8) then ys1 = -2 end
	if dst(o,e) < 27 then e.x += xs1*8 e.y += ys1*8 end
	
	end
	
	
	if flr((o.x)/8) < flr((a.x)/8) then xs = 2 end
	if flr((o.y)/8) < flr((a.y)/8) then ys = 2 end
	if flr((o.x)/8) > flr((a.x)/8) then xs = -2 end
	if flr((o.y)/8) > flr((a.y)/8) then ys = -2 end
	if dst(o,a) < 27 then blast(xs,ys) end
	make_particle(o.x,o.y,1)
	del(towers,o)
	end
	end
	
	
	
	if o.t == "t" then 
	local random = flr(rnd(30) + 1)
	if random == 5 then o.side = false end
	if random==12 then o.side = true end
	if random == 1	then o.side2 = true  end
	if random == 2 then o.side2 = false end	
	if random == 6 then tank_shoot(o.x,o.y,o.side) end
	end

end

function blast(b,by)
for s in all(area) do s.y+=by*8 s.x+= b*8 end
a.x += b*8
a.y += by*8
end


function make_particle(x,y,c)
local p = {}
p.x = x
p.y = y
p.s = 5
p.c = c
add(particles,p)
return p
end

function update_particle(o)
o.s += .5 
if o.s > 10 then del(particles,o) 
end
end



function check_menu()
if(
(tilex == 15 and tiley == 1) or 
(tilex == 14 and tiley == 1) or 
(tilex == 13 and tiley == 1) or 
(tilex == 13 and tiley == 1) or 
(tilex == 12 and tiley == 1) or	

(tilex == 15 and tiley == 2) or 
(tilex == 14 and tiley == 2) or 
(tilex == 13 and tiley == 2) or 
(tilex == 13 and tiley == 2) or 
(tilex == 12 and tiley == 2) or	

(tilex == 15 and tiley == 3) or 
(tilex == 14 and tiley == 3) or 
(tilex == 13 and tiley == 3) or 
(tilex == 13 and tiley == 3) or 
(tilex == 12 and tiley == 3) or	
 
(tilex == 15 and tiley == 4) or 
(tilex == 14 and tiley == 4) or 
(tilex == 13 and tiley == 4) or 
(tilex == 13 and tiley == 4) or 
(tilex == 12 and tiley == 4) or
(tilex == 15 and tiley == 0))
then
return false
end

return true
end



function dst(o1,o2)
 return sqrt(sqr(o1.x-o2.x)+sqr(o1.y-o2.y))
end

function sqr(x) return x*x end


function tank_shoot(x,y,s)
local b = {}
b.x = x
b.y = y
b.w = 2
b.h = 2
b.spre = 0.3
b.dx = rnd(3)+1
b.dy = rnd(b.spre) -b.spre/2
if s == false then b.side = true b.dx = -b.dx end
if s == true then b.side = false b.dx =b.dx end
add(bullets_e,b)
return b
end
