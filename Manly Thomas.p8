function _update()
if starting_screen then
if btn(5) then pause = true starting_screen = false end
end
if not starting_screen then
					for e in all(enemies) do
					if cmap(e) then del(enemies,e) end
					if (e.x < 0 or e.x > 120 or e.y < 0 or e.y > 128) then del(enemies,e) end
					end

					for ar in all(area) do 
					for b in all(blocks) do
					if (ar.cando == true and collide(b,ar)) then  
								make_particle(ar.x,ar.y,7)		
					end end
					end
					
					if btnp(4) then starting_screen = true end
     for e in all(enemies) do if e.health <= 0 then del(enemies,e) end end
     
     for b in all(bullets_e) do 
     b.x += b.dx
     b.y += b.dy
     if (b.x > 128 or b.x < 0 or b.y > 128 or b.y < 0) then del(bullets_e,b) end
     			for e in all(enemies) do
     			 if collide(e,b) then make_particle(e.x+flr(rnd(4)-2),e.y+flr(rnd(4)-2),8) e.health -= 1 	del(bullets_e,b) end
     			end
     
     end
     
     if (a.x>120 or a.x<8 or a.y < 8 or a.y > 120) then health = 0 end
     
     foreach(particles,update_particle)
     
     if health <= 0.5 then health = 0 end
     
     for b in all(bullets) do
     if collide(a,b) then health -= .25 sfx(1) del(bullets,b) end
     end
     
     foreach(towers,update_towers)
     for t in all(towers) do 
      for b in all(blocks) do
      if collide(b,t) then del(blocks,b) end
      end
     end
     
     
     tilex = flr(stat(32)/8)
     tiley = flr(stat(33)/8)
     
     if stat(34) ==  0 then menu_clicked2 = false menu_clicked = false end 
     if (stat(34) == 1 and menu == true and (tilex == 12 or tilex == 13) and (tiley==1 or tiley==0)) then chose = "b" end
     if (stat(34) == 1 and menu == true and (tilex == 12 or tilex == 13) and tiley==2) then chose = "t" end
     
     if(menu == false and menu_clicked == false and tilex == 15 and tiley == 0 and stat(34)==1) then
     menu = true
     menu_clicked = true
     end
     
     if(menu_clicked == false and menu == true and tilex == 15 and tiley == 0 and stat(34)==1) then
     menu = false
     menu_clicked = true
     end
     
     
     if (menu_clicked2 == false  and mouse_block() and stat(34)==1 and menu == true) then
     menu_clicked2 = true
     if (check_menu() and money >= 100 and chose == "b") then make_tower(tilex*8,tiley*8,chose) money -= 100 end
     if (check_menu() and money >= 100 and chose == "t") then make_tower(tilex*8,tiley*8,chose) money -= 100 end
     end 
     
     
     
     
     for a in all(area) do 
     if (a.x < 0 or a.x > 128 or a.y < 0 or a.y > 128) then a.cando = false end
     end
     
     if check_area() then health = 0 end
     for b in all(bullets) do 
     b.x += b.dx
     b.y += b.dy
     end
     
     foreach(enemies,enemy_update)
     if health <= 0 then highscore = level level = 1 _init() end
     
     for e in all(enemies) do
     	for b in all(blocks) do
     	if collide(b,e) then del(blocks,b)end
     	end
     end
     
     -----------
     if tr.text != nil then tr.text += 1 end
     if tr.text != nil then if tr.text > 90 then tr.text = nil end end
     for b in all(blocks) do
     if collide(f,b) then del(blocks,b) end
     if collide(a,b) then del(blocks,b) end
     end
     -------------
     
     
     
     if stat(34)==0 then a.timer = false end
     
     if a.timer == false then
      for ar in all(area) do
      		
      		for b in all(blocks) do
      			if (collide(b,ar) or cmap(ar)) then ar.cando = false end
      		end
      	lx = a.x
      	ly = a.y
      	if (menu == false and collide_mouse(ar,1) and stat(34) == 1 and ar.cando) then a.x = ar.x a.y = ar.y 
      		a.timer = true
      		
      		for area in all(area) do 
      			area.x += a.x - lx
      			area.y += a.y - ly
      			if a.x - lx < 0 then a.side = true else a.side = false end
      		end 
      	end
      end
     end
     
     
     t += 1
     if t%2 == 0 then
     score += 1
     end
     if t == 60 then t = 0 end
     foreach(blocks,block_update)
     move(r ,b)
     move(l ,b)
     
     for b in all(balls) do
     for bl in all(blocks) do
     if collide(b,bl) then sfx(0) bl.health -= 1 b.dx  = -b.dx b.dy = -b.dy end
     end
     if collide(b,a) then
     b.dx = -b.dx
     health -= 1
     sfx(1)
     b.dy = -b.dy
     end
     if collide(l,b) then 
     b.dx = -b.dx
     b.dy = b.dy
     end
     if collide(r,b) then 
     b.dx = -b.dx
     b.dy = b.dy
     end
     b.x += b.dx
     b.y += b.dy
     if b.x < 10 then b.dx = 2 end
     if b.y < 10 then b.dy = 2 end
     if b.x > 110 then b.dx = -2 end
     if b.y > 110 then b.dy = -2 end
     end
     
     if collide(f,a) then sfx(5) level+= 1 _init() end
	end
